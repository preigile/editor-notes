<div class='row'>
    <div class='col-sm-3'>
        <p>There are {{notesCount}} notes</p>
        <div class='list-group'>
            {{#each}}
                {{#link-to 'note' this classNames='list-group-item'}}
                    {{title}}
                {{/link-to}}
            {{/each}}
        </div>
    </div>
    <div class='col-sm-9 note-presentation'>
        {{outlet}}
    </div>
</div>
